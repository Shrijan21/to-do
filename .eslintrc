{
    "extends": ["prettier"],
    "plugins": ["prettier"],
    "env": {
      "browser": true,
      "webextensions": true
    },
    "rules": {
      "prettier/prettier": ["error", {
        "endOfLine":"auto"
      }]
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6
    }
  }